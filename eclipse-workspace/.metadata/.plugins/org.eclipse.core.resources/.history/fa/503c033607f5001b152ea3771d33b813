package markcalculation;

public class Marks {

	public static void main(String[] args) {
		 
		int[] sub = new int[10];
		sub[0] = 56;
		sub[1] = 92;
		sub[2] = 84;
		sub[3] = 65;
		sub[4] = 95;
		float total = 0;;
		float avg;
		
		
		
		for(int i=0;i<sub.length;i++) {
			total = total + sub[i];
		}
		
		avg = total / sub.length;
		
		System.out.println("The average score of the student is : "+avg);
				
	}

}
